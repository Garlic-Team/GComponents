{
  "name": "@gcommands/components",
  "version": "1.1",
  "description": "Component handler for discord.js and gcommands",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bash build.bash",
    "babelbuild": "npx babel --extensions .ts ./src -d ./dist --minified",
    "tscbuild": "tsc --emitDeclarationOnly",
    "docs:build": "typedoc --json docs/out-main.json src/index.ts && node scripts/docs.mjs",
    "docs:devbuild": "typedoc --json docs/out-dev.json src/index.ts && node scripts/devdocs.mjs"
  },
  "keywords": [
    "handler",
    "discord bot"
  ],
  "author": "Garlic-Team",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/garlic-team/gcomponents.git"
  },
  "bugs": {
    "url": "https://github.com/Garlic-Team/GComponents/issues"
  },
  "dependencies": {
    "discord.js": "^13.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@discordjs/ts-docgen": "^0.3.2",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "eslint": "^8.1.0",
    "glob": "^7.2.0",
    "typescript": "^4.4.3"
  },
  "homepage": "https://garlic-team.github.io/GComponents"
}
